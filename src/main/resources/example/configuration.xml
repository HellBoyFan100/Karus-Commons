<?xml version = "1.0" encoding = "UTF-8"?>

<!-- Example configuration.xml -->

<!-- Mandatory DTD to parse the document -->
<!DOCTYPE commands SYSTEM "configuration.dtd">


<commands>
  
  <!-- 
  Commands can only have either child commands defined by the element: <commands> or 
  by the various configuration elements, e.g <cooldown> and <worlds>
  -->
  <command name = "command">
  
    <commands>
      
      <command name = "subcommand">
        
        <configuration>
          <cooldown time = "100"/>
          <worlds type = "whitelist" list = "world1, wordl2"/>
        </configuration>
        
      </command>
      
    </commands>
    
  </command>
  
</commands>